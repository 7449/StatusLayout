        <attr name="status_error_layout" format="integer"/>
        <attr name="status_success_layout" format="integer"/>
        <attr name="status_empty_layout" format="integer"/>
        <attr name="status_loading_layout" format="integer"/>
        <attr name="status_normal_layout" format="integer"/>
